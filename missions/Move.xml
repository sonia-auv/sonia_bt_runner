<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="Untitled">
    <Sequence>
      <InitTraj trajectory="{trajPosX}"/>
      <AddPoseToTraj positionX="0.2" trajectoryIn="{trajPosX}" trajectoryOut="{trajPosX}"/>
      <AddPoseToTraj positionX="0.4" trajectoryIn="{trajPosX}" trajectoryOut="{trajPosX}"/>
      <SendTrajToPlanner trajectory="{trajPosX}"/>
    </Sequence>
  </BehaviorTree>

  <TreeNodesModel>
        <Action ID="AddPoseToTraj">
            <output_port name="trajectoryOut" type="Trajectory"/>
            <input_port name="precision" type="int">0</input_port>
            <input_port name="speed" type="int">0</input_port>
            <input_port name="orientationZ" type="float">0.0</input_port>
            <input_port name="orientationY" type="float">0.0</input_port>
            <input_port name="orientationX" type="float">0.0</input_port>
            <input_port name="frame" type="int">1</input_port>
            <input_port name="positionZ" type="float">0.0</input_port>
            <input_port name="trajectoryIn" type="Trajectory"/>
            <input_port name="longRotation" type="bool">false</input_port>
            <input_port name="positionY" type="float">0.0</input_port>
            <input_port name="positionX" type="float">0.0</input_port>
        </Action>
        <Action ID="DropperActionNode">
            <input_port name="side" type="std::string"/>
        </Action>
        <Action ID="InitTraj">
            <output_port name="trajectory" type="Trajectory"/>
        </Action>
        <Condition ID="MissionSwitchStatus"/>
        <Action ID="SendTrajToPlanner">
            <input_port name="trajectory" type="Trajectory"/>
            <input_port name="interpolation" type="int">0</input_port>
        </Action>
        <Action ID="WaitTargetReached">
            <input_port name="timeout" type="int">5</input_port>
        </Action>
    </TreeNodesModel>
</root>
